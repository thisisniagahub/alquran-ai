{
  "version": 2,
  "name": "alquran-ai",
  "builds": [
    {
      "src": "backend/server.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/server.py"
    },
    {
      "src": "/(.*)",
      "dest": "backend/server.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}
